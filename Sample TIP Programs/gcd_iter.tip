main(n1, n2) {
  var sentinel;
  sentinel = 0; 
  if (n2 == 0) {
    sentinel = 1;
  }
  if (n1 == 0) {
     n1 = n2;
     sentinel = 1;
  }
  while (sentinel == 0) {
     if (n1 > n2) {
        n1 = n1 - n2;
     } else {
        n2 = n2 - n1;
     }

     if (n2 == 0) {
        sentinel = 1;
     }
  }
  return n1;
}
