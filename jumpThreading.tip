//   time ./flatten 10000
main(n) {
  var i, r, v;
  i = 0;
  v = 42;
  while (n > i) {
    r = jumpThreading(n, v);
    i = i + 1;
  }
  return 0;
}

jumpThreading(n, v) {
  var i, j, r;
  i = 0;
  r = 0;
  //r = r + (v*v-v*v+v*v+v/2*v-v*v);
  while (n > i) {
    if (r == 0) {
      r = 1;
    }
    if (r == 0) {
      r = r + (v*v-v*v+v*v+v/2*v-v*v);
    } else {
      r = 0;
    }
    i = i + 1;
  }
  return n;
}