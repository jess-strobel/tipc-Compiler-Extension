/*
 * Benchmark for loop unroll and jamming optimization.
 * 
 * Compile with and without the -jam optimization then run:
 *   time ./jam 20000
 * to observe the execution time of the benchmark.
 */
main(n) {
  var i, j, r, v;
  i = 0;
  j = 0;
  v = 42;
  while (n > i) {
    r = unroll(n);
    i = i + 1;
  }
  while (n > j) {
    r = unroll(n);
    j = j + 1;
  }
  return 0;
}

unroll(n) {
  var i;
  i = 0;
  while (n > i) {
    i = i + 1;
  }
  return n;
}